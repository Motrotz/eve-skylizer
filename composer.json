{
    "name": "rtgquack/skylizer",
    "description": "sk≈∏lizer: the Eve-Online Scan Analyzer",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "eve-online",
        "api",
        "laminas"
    ],
    "homepage": "https://skylizer.eve-tools.info",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-gd": "*",
        "ext-intl": "*",
        "laminas/laminas-component-installer": "^2",
        "laminas/laminas-mvc": "^3.0.1",
        "laminas/laminas-development-mode": "^3.0",
        "laminas/laminas-mvc-form": "^1.0",
        "laminas/laminas-mvc-plugins": "^1.0.1",
        "laminas/laminas-session": "^2.7.1",
        "laminas/laminas-authentication": "^2.5",
        "doctrine/migrations": "^3",
        "laminas/laminas-math": "^3.0",
        "laminas/laminas-crypt": "^3.1",
        "laminas/laminas-captcha": "^2.6",
        "doctrine/doctrine-orm-module": "^4",
        "laminas/laminas-cache": "^2.7",
        "laminas/laminas-serializer": "^2.8",
        "laminas/laminas-permissions-rbac": "^2.5",
        "laminas/laminas-log": "^2.9",
        "laminas/laminas-cli": "^1",
        "laminas/laminas-dependency-plugin": "^2.0",
        "tasmaniski/laminas-current-route": "dev-master",
        "beberlei/doctrineextensions": "^1.0",
        "evelabs/oauth2-eveonline": "^1.1",
        "eveseat/eseye": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "User\\": "module/User/src/",
            "VposMoon\\": "module/VposMoon/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "UserTest\\": "module/User/test/",
            "VposMoon\\": "module/VposMoon/test/"
        }
    },
    "extra": [],
    "scripts": {
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    },
    "require-dev": {
        "laminas/laminas-developer-tools": "^2",
        "san/san-session-toolbar": "^4",
        "phpunit/phpunit": "^9"
    }
}
